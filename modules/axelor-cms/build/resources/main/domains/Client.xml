<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.0.xsd">

    <module name="cms" package="com.axelor.cms.db"/>

    <entity name="Client" table="clients" repository="none">
        <string name="firstName" required="true"/>
        <string name="lastName" required="true"/>
        <integer name="age" required="true"/>
        <date name="dateOfBirth" required="true"/>
        <string name="country" required="true"/>
    </entity>

</domain-models>